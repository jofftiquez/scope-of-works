{
  "name": "scope-of-works",
  "version": "0.0.3",
  "description": "Math library for construction scope of works.",
  "main": "index.js",
  "semistandard": {
    "parser": "babel-eslint",
    "globals": [
      "test",
      "expect",
      "jest",
      "describe",
      "beforeEach"
    ],
    "ignore": [
      "webpack.config.js",
      "launch.js"
    ]
  },
  "jest": {
    "verbose": true,
    "modulePathIgnorePatterns": [
      "/node_modules/",
      "/vendor/",
      "/dist/",
      "/node_modules/tns-core-modules/"
    ],
    "moduleFileExtensions": [
      "js",
      "vue"
    ]
  },
  "scripts": {
    "test": "jest --watchAll --coverage",
    "lint": "semistandard --fix | snazzy",
    "tester": "node tester.js",
    "start": "webpack --watch",
    "prebuild": "rm -rf dist",
    "build": "webpack --progress --hide-modules"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.4",
    "semistandard": "^12.0.1",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jofftiquez/scope-of-works.git"
  },
  "keywords": [
    "es5",
    "es6",
    "babel",
    "webpack",
    "rollup"
  ],
  "author": "Jofferson Ramirez Tiquez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jofftiquez/scope-of-works/issues"
  },
  "homepage": "https://github.com/jofftiquez/scope-of-works#readme",
  "dependencies": {
    "commitizen": "^2.10.1",
    "jest": "^23.6.0",
    "snazzy": "^8.0.0",
    "tape": "^4.9.1"
  }
}
